pipeline {
agent any 
stages {
   stage ('compile stage') {
   steps {
    sh 'mvn compile'
    }
   }
   stage ('test stage') {
   steps {
   sh 'mvn test'
     }
   }
   stage ('package') {
   steps {
      sh 'mvn package'
      }
   }
   post {
   success {
     archiveArtifacts artifacts: 'pom.xml', fingerprint: true
     }
   }
 }
}
